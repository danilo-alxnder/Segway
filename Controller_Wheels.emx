       implementation eq
// 20-sim Linear System Editor
// tf matlab C h 0.001
parameters
	real hidden A [1, 1] = [0.895170030885073];
	real hidden B [1, 1] = [1.25177201023831];
	real hidden C [1, 1] = [-2.24286129247643];
	real hidden D [1, 1] = [27.1865602463161];
	real hidden x0 [1] = [0.0];
variables
	real x [1];
equations
	next (x, x0) = A * x + B * u;
	y = C * x + D * u;
