</Description>';
    type 'WaveGenerator-VariableTimeSquare'
     ports
      signal out output;
    end;
    icon bg bottom
     figures
      rectangle 176 200 208 232 color 0 fill 15132390;
      line 179.8 204.5 179.9 230.2 color 0 fill 0;
      line 177.9 228.1 203.9 228.1 color 0 fill 0;
      line 179.3 228.1 186 228 186 216 color 16711680 fill 0;
      line 190.7 228.1 190.7 216 190.7 228.1 color 16711680 fill 0;
      line 200 216 200 228.1 200 216 color 16711680 fill 0;
      line 204.2 216.1 200 216.1 color 16711680 fill 0;
      line 191.5 228.2 199.5 228.2 color 16711680 fill 0;
      line 204.2 228.4 204.2 216.3 204.2 228.4 color 16711680 fill 0;
      line 190.4 216.2 186.2 216.2 color 16711680 fill 0;
    end;
    implementation eq
parameters
	real interval = 1 {s};
	real pulselength = 0.5 {s};
	real amplitude = 1.0;			// amplitude of the pulse
variables
	real hidden x;
	real hidden nextTime;
	boolean hidden timetrigger;
initialequations
	if pulselength >= interval then stopsimulation ('pulselength should be smaller than interval'); end;
	x = 1.0;
	nextTime = starttime;
code
	timetrigger = timeevent(nextTime);

	if( time >= nextTime ) then
		
		// give the negated value of x to the output
		if x == 0.0 then
			output = 1.0;
		else
			output = 0.0;
		end;

		// only change x on major intervals
		if major then
			x = output;

			// and determine the next time
			if( x == 0.0 ) then
				nextTime = time + interval - pulselength;
			else
				nextTime = time + pulselength;
			end;

		end;

	else
		// just copy x to the output
		output = amplitude * x;
	end;
implementation_end;
  end;
